<odoo>
    <data noupdate="1">
        <record id="temporary_sequence" model="ir.sequence">
            <field name="name">Temporary Sequence</field>
            <field name="code">temporary.sequence</field>
            <field name="active">TRUE</field>
            <field name="prefix">TC</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="customer_code_sequence" model="ir.sequence">
            <field name="name">Customer Code</field>
            <field name="code">customercode.sequence</field>
            <field name="active">TRUE</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>