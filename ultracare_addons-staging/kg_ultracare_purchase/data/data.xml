<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">
        <record id="vendor_code_sequence" model="ir.sequence">
            <field name="name">Vendor Code</field>
            <field name="code">vendorcode.sequence</field>
            <field name="active">TRUE</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="kg_seq_request_for_quotation" model="ir.sequence">
            <field name="name">Request For Quotation</field>
            <field name="code">kg.req.for.quot</field>
            <field name="prefix">RFQ</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">3</field>
        </record>
        <record id="kg_seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order</field>
            <field name="code">kg.purchase.order.seq.code</field>
            <field name="prefix">P</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
         <record id="kg_seq_purchase_requisition" model="ir.sequence">
            <field name="name">Purchase Requisition</field>
            <field name="code">kg.purchase.requisition.seq.code</field>
            <field name="prefix">PA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>